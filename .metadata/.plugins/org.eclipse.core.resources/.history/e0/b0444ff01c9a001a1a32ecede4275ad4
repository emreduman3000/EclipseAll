package codes;


public class studentAnswers
{
	

	public static void main(String[] args)
	{	
		int [ ] [ ] matrxx = { {1,2}, {4,5,}, {4,6}, {4,5,6,7,8,9} };

		//Print 2d array
	    for (int i=0; i<matrxx.length; i++)
	    {
	        for (int ji=0; ji<matrxx[ji].length;ji++)
	        {
	            System.out.print (matrxx[i][ji] + "\t");
	        }
	            System.out.println();
	    }

	    System.out.println ("matrix.length = " + matrxx.length );
	    System.out.println ("matrix[3].length = " + matrxx[3].length );
		
		    
		    
		    
	    int arr2[][]={{1,2},{5},{6,7,8}};
		int sum1=0;
		
		for (int[] w:arr2)
		{
			for (int z:w)
			{
				System.out.print(z+" ");
				sum1=sum1+z;
			}		
			System.out.println("");
		}
		
		System.out.println(sum1);		
	}
	
}
