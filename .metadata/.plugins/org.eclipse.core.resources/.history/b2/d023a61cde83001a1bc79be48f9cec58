package first;

public class firstt {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		

		//day1
		//syso or sout or sysout +CTRL+SPACE
		
		System.out.println("EMRE ");
		System.out.println();
		char c='!'; int cc='!';
	
		System.out.println(c+"\n"+cc);
		
		
		
		//day2
		
		//find min and max values of int data type and print on the console.
		//Wrapper class of int is Integer.It has lots of methods.
		System.out.println("Min:"+Integer.MIN_VALUE+"  Max:"+Integer.MAX_VALUE);
		System.out.println("Size:"+Long.SIZE+"  Bytes:"+Long.BYTES);
		System.out.println("Size:"+Short.parseShort(null)+"  Bytes:");
		System.out.println("Size:"+Short.MIN_VALUE+"  Bytes:"+Short.MAX_VALUE);
		
	}
	

}
