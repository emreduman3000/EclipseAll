package first;

import java.util.Scanner;

public class firstt {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//INFOs
		
		//syso or sout or sysout +CTRL+SPACE
		//system class, out method,
		
		int n1=10,n2=15;
		int ekok = ekokBul(n1,n2);
		System.out.println("ekok:"+ekok);
		ebobBul(n1,n2);
	        
    }
	
	
	
	
	
	/*
	 * return ekok
	 */
	private static int ekokBul(int num1, int num2) 
	{
		// TODO Auto-generated method stub
		
		int ekok =(num1 * num2) / ebobBul(num1,num2);
		return ekok;
	}
	
	

	private static int ebobBul(int num1, int num2)	
	{
		// TODO Auto-generated method stub
		int ebob=1;
		int control=2;
		while(control <= num1 && control <=num2)
		{
			if(num1%control==0 && num2%control==0)
			{
				ebob=control;					
			}
			control++;
		}
		if (ebob==1)
			System.out.println("One of them or both numbers are prime ");
		else
			System.out.println("Ebob is:"+ebob);
			
		return ebob;
	}



}
	


